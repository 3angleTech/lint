    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES2015",
    "lib": [
      "ES2020"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ]
  }
}
